{
  "name": "api",
  "$schema": "https://github.com/nrwl/nx/raw/master/packages/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "apps/api/src",
  "targets": {
    "build": {
      "executor": "@odnlabs/nx-rust:build",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "executor": "@odnlabs/nx-rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@odnlabs/nx-rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api"
      }
    },
    "format": {
      "executor": "@odnlabs/nx-rust:format",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api"
      }
    },
    "install": {
      "executor": "@odnlabs/nx-rust:install",
      "outputs": ["{options.path}"],
      "options": {
        "path": "apps/api"
      }
    },
    "dev": {
      "executor": "@odnlabs/nx-rust:watch",
      "options": {
        "watch": "apps/api/src"
      }
    },
    "run": {
      "executor": "@odnlabs/nx-rust:run",
      "options": {
        "verbose": true
      }
    }
  },
  "tags": []
}
